@{
    ViewBag.Title = "Prin tApproved Purchase Indent";
    Layout = "~/Views/Shared/_PurchaseLayout.cshtml";
}
<link href="~/Content/jquery-ui.css" rel="stylesheet" />
<div class="row top-head">
    <div class="col-md-12"><p><b><a style="color:#fff;" href="~/Dashboard/PurchaseDashboard">Purchase</a> >> Approved Purchase Indent</b></p></div>
</div>
<section>
    <form action="#" id="frmAddEditQuotation" class="form-horizontal as" enctype="multipart/form-data" method="post" accept-charset="utf-8">
        <div class="row inner">
            <div class="message">
            </div>
            <div class="col-md-12 ftop">
                <div class="form-group col-md-4">
                    <label for="txtIndentNo" class="col-sm-4 control-label">Indent No.</label>
                    <div class="col-sm-8">
                        <input class="form-control" id="txtIndentNo" name="txtIndentNo" tabindex="1" placeholder="Search With Indent No." type="text" maxlength="100" />                
                        <input id="hdnAccessMode" type="hidden" value="@ViewData["accessMode"]" />
                    </div>
                </div>
                <div class="form-group col-md-4">
                    <label for="txtSearchPONo" class="col-sm-4 control-label">PO No.</label>
                    <div class="col-sm-8">
                        <input class="form-control" id="txtSearchPONo" name="txtSearchPONo" tabindex="2" placeholder="Search With PO No." type="text" maxlength="100" />
                       
                    </div>
                </div>
                <div class="form-group col-md-4">
                    <label for="txtVendorName" class="col-sm-4 control-label">Vendor Name</label>
                    <div class="col-sm-8">
                        <input class="form-control" id="txtVendorName" name="txtVendorName" tabindex="3" placeholder="Search With Vendor Name" type="text" maxlength="100" />                        
                    </div>
                </div>
            </div>
          
           
            <div class="col-md-12">
                <div class="form-group col-md-4">
                    <label for="txtFromDate" class="col-sm-4 control-label">Indent From Date</label>
                    <div class="col-sm-8 ">
                        <input class="form-control" id="txtFromDate" name="txtFromDate" tabindex="4" value="@ViewData["fromDate"]" type="text" maxlength="50" />
                        <input id="hdnFromDate" name="hdnFromDate" value="@ViewData["fromDate"]" type="hidden" />
                    </div>
                </div>
                <div class="form-group col-md-4">
                    <label for="txtToDate" class="col-sm-4 control-label">To Date</label>
                    <div class="col-sm-8 ">
                        <input class="form-control" id="txtToDate" name="txtToDate" tabindex="5" value="@ViewData["toDate"]" type="text" maxlength="50" />
                        <input id="hdnToDate" name="hdnFromDate" value="@ViewData["toDate"]" type="hidden" />
                    </div>
                </div>
                <div class="form-group col-md-4">
                    <label for="ddlCompanyBranch" class="col-sm-4 control-label">Company Branch</label>
                    <div class="col-sm-8">
                        <select id="ddlCompanyBranch" name="ddlCompanyBranch" tabindex="46" class="form-control"></select>
                        <input id="hdnBillingStateId" type="hidden" value="0" />
                        <input id="hdnSessionCompanyBranchId" type="hidden" value="@ViewData["CompanyBranchId"]" />
                        <input id="hdnSessionUserID" type="hidden" value="@ViewData["UserId"]" />
                    </div>
                </div>
            </div>
            <div class="row col-sm-12 col-lg-12 col-md-12">
                <div class="col-md-3"></div>
                <div class="col-md-6">
                    <center>
                     
                        <a class="btn btn-success" alt="Export" tabindex="6" id="btnExport" onclick="OpenPrintPopup();" border="0">Generate Report</a>
                        <input class="btn btn-success" type="button" id="btnReset" tabindex="7" onclick="ClearFields()" value="Reset" />                       
                    </center>
                </div>
                <div class="col-md-3"></div>
            </div>
           
        </div>
        <!-- Modal -->
        <div class="modal fade" id="alertModel" role="dialog">
            <div class="modal-dialog  modal-sm">
                <!-- Modal content-->
                <div class="modal-content">
                    <div class="modal-header">
                        <button type="button" class="close" data-dismiss="modal">&times;</button>
                        <h4 class="modal-title" id="modelHeader"></h4>
                    </div>
                    <div class="modal-body">
                        <p id="modelText"></p>
                    </div>
                    <div class="modal-footer">
                        <button type="button" class="btn btn-primary  btn-block" data-dismiss="modal">Close</button>
                    </div>
                </div>

            </div>
        </div>
        <div class="modal fade" id="printModel" role="dialog">
            <div class="modal-dialog  modal-sm">
                <!-- Modal content-->
                <div class="modal-content">
                    <div class="modal-header">
                        <button type="button" class="close" data-dismiss="modal">&times;</button>
                        <h4 class="modal-title">Approved Indent Report</h4>
                    </div>
                    <div class="modal-body">
                        <div class="form-group">
                            <label for="ddlPrintOption" class="col-sm-4 control-label">Report Option</label>
                            <div class="col-sm-8">
                                <select id="ddlPrintOption" name="ddlPrintOption" onchange="ShowHidePrintOption()" tabindex="1" class="form-control">
                                    <option selected="selected" value="PDF">PDF</option>
                                    <option value="Excel">Excel</option>
                                </select>
                            </div>
                        </div>
                    </div>
                    <div class="modal-footer">
                        <a class="btn btn-success" target="_blank" href="#" id="btnPdf">Print</a>
                        <a class="btn btn-success" style="display:none;" target="_blank" href="#" id="btnExcel">Print</a>
                        <button type="button" class="btn btn-primary" data-dismiss="modal">Close</button>

                    </div>
                </div>

            </div>
        </div>
        
     
    </form>
</section>
<script src="~/Scripts/PurchaseIndent/ListApprovedIndent.js"></script>
<script src="~/Scripts/jquery-ui.min.js"></script>
<script src="~/Scripts/paging.js"></script>
