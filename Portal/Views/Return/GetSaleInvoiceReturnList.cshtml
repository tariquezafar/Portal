@model List<Portal.Core.ViewModel.WarrantyViewModel>
<script src="~/Scripts/jquery.tablesorter.js"></script>

<style>
    .table th {
        background: rgba(0, 0, 0, 0) linear-gradient(#375CB0, #6e96e2) repeat scroll 0 0;
    }

    .table th {
        color: #ffffff;
        text-align: center;
    }

    .table tr {
        text-align: center;
    }
</style>

@if (Model != null && Model.Count > 0)
{
    <div style="overflow-x:scroll; padding:10px;background-color:white;">
        <table class="table table-bordered table-hover" id="tblJobWorkList">
            <thead>
                <tr>
                    <th align="center">Select</th>
                    <th><center>Sale Invoice No.</center></th>
                    @*<th><center>PackingList No.</center></th>
                    <th><center>PackingList Date</center></th>*@
                    <th><center>Dispatch Date</center></th>                                    
                </tr>
            </thead>
            <tbody>
                @foreach (var item in Model)
                {
                    <tr class="success">

                        <td style="width:30px; margin:0px 2px auto;">
                            <a class="fa fa-pencil-square-o erp-btn" href="#" onclick="SelectSI('@item.WarrantyID','@item.InvoiceId','@item.InvoiceNo','@item.InvoicePackingListNo')"></a>
                        </td>                       
                        <td>@item.InvoiceNo</td>
                        @*<td>@item.InvoicePackingListNo</td>
                        <td>@item.InvoicePackingListDate</td>*@                       
                        <td style="text-align:left;">@item.DispatchDate</td>                        
                    </tr>
                }

            </tbody>
        </table>
    </div>
}
else
{
    <h3 style="color:firebrick;text-align:center">Records not Found!!!</h3>
}
<script>
    $(document).ready(function () {
        $('#tblJobWorkList').paging({
            limit: 10,
            rowDisplayStyle: 'block',
            activePage: 0,
            rows: []
        });

    });
</script>
<style>
    .btn-success a {
        background-color: #446abc !important;
        border-color: #233e79 !important;
        display: -webkit-inline-box;
        padding: 5px;
    }
</style>