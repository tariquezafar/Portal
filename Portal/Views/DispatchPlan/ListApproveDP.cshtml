@{
    ViewBag.Title = "ListDispatchPlan";
    Layout = "~/Views/Shared/_SaleLayout.cshtml";
}
<link href="~/Content/jquery-ui.css" rel="stylesheet" />

<div class="row top-head">
    <div class="col-md-12"><p><b><a style="color:#fff;" href="~/Dashboard/AdminDashboard">Admin</a> >> Approve Dispatch Plan </b></p></div>
</div>
<section>
    <form action="#" id="frmAddEditComplaintService" class="form-horizontal as" enctype="multipart/form-data" method="post" accept-charset="utf-8">
        <div class="row inner">
            <div class="message">

            </div>
            <div class="col-md-12 ftop">
                <div class="form-group  col-md-4">
                    <label for="txtDispatchPlanNo" class="col-sm-4 control-label">Dispatch Plan No.</label>
                    <div class="col-sm-8">
                        <input class="form-control" id="txtDispatchPlanNo" name="txtDispatchPlanNo" tabindex="1" placeholder="Search with Dispatch Plan No." type="text" maxlength="100" />
                    </div>
                </div>
                <div class="form-group col-md-4">
                    <label for="txtCustomerName" class="col-sm-4 control-label">Customer Name&nbsp;</label>
                    
                        <div class="col-sm-8">
                            <input class="form-control" id="txtCustomerName" name="txtCustomerName" tabindex="1"  placeholder="Search with Customer Name" type="text" maxlength="100" />
                        </div>
                </div>

                <div class="form-group col-md-4">
                    <label for="ddlCompanyBranch" class="col-sm-4 control-label">Company Branch</label>
                    <div class="col-sm-8">
                        <select id="ddlCompanyBranch" name="ddlCompanyBranch" tabindex="5" class="form-control"></select>
                        <input id="hdnSessionCompanyBranchId" type="hidden" value="@ViewData["CompanyBranchId"]" />
                        <input id="hdnSessionUserID" type="hidden" value="@ViewData["UserId"]" />

                    </div>
                </div>

            </div>
            
            <div class="col-md-12">

                <div class="form-group col-md-4">
                    <label for="ddlApprovalStatus" class="col-sm-4 control-label">Status</label>
                    <div class="col-sm-8">
                        <select id="ddlApprovalStatus" name="ddlApprovalStatus" tabindex="80" class="form-control">
                            <option selected="selected" value="0">Select Status</option>
                            <option value="Final">Final</option>
                            <option value="Approve">Approve</option>
                        </select>

                    </div>
                </div>

                <div class="form-group col-md-4">
                    <label for="txtSearchFromDate" class="col-sm-4 control-label">SO From Date</label>
                    <div class="col-sm-8 ">
                        <input class="form-control" id="txtSearchFromDate" name="txtSearchFromDate" tabindex="17" value="@ViewData["fromDate"]" type="text" maxlength="50" />
                        <input id="hdnFromDate" name="hdnFromDate" value="@ViewData["fromDate"]" type="hidden" />
                    </div>
                </div>
                <div class="form-group col-md-4">
                    <label for="txtSearchToDate" class="col-sm-4 control-label">To Date</label>
                    <div class="col-sm-8 ">
                        <input class="form-control" id="txtSearchToDate" name="txtSearchToDate" tabindex="18" value="@ViewData["toDate"]" type="text" maxlength="50" />
                        <input id="hdnFromDate" name="hdnFromDate" value="@ViewData["toDate"]" type="hidden" />
                    </div>
                </div>

            </div>

            <div class="row col-sm-12 col-lg-12 col-md-12">
                <div class="col-md-3"></div>
                <div class="col-md-6">
                    <center>
                        <input class="btn btn-success" value="Search" alt="Save" tabindex="7" id="btnSearch" onclick="SearchApproveDispatchPlan()" type="button" border="0">&nbsp;&nbsp;
                        <input class="btn btn-success" type="button" id="btnReset" tabindex="8" onclick="ClearFields()" value="Reset" /> &nbsp;&nbsp;
                        <a class="btn btn-success" id="btnList" tabindex="9" href="@Url.Content("~/DispatchPlan/AddEditDispatchPlan?accessMode=1")">Add More Dispatch Plan</a>
                    </center>
                </div>
                <div class="col-md-3"></div>
            </div>
            <div class="row col-sm-6 col-lg-12 col-md-12">
                <div id="divList">

                </div>
            </div>
        </div>

        <!-- Modal -->
        <div class="modal fade" id="alertModel" role="dialog">
            <div class="modal-dialog  modal-sm">
                <!-- Modal content-->
                <div class="modal-content">
                    <div class="modal-header">
                        <button type="button" class="close" data-dismiss="modal">&times;</button>
                        <h4 class="modal-title" id="modelHeader"></h4>
                    </div>
                    <div class="modal-body">
                        <p id="modelText"></p>
                    </div>
                    <div class="modal-footer">
                        <button type="button" class="btn btn-primary  btn-block" data-dismiss="modal">Close</button>
                    </div>
                </div>

            </div>
        </div>
    </form>
</section>
<script src="~/Scripts/DispatchPlan/ListDispatchPlan.js"></script>
<script src="~/Scripts/jquery-ui.min.js"></script>
<script src="~/Scripts/paging.js"></script>
<script src="~/Scripts/Common.js"></script>
