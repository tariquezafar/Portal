@{
    ViewBag.Title = "List SOSetting";
    Layout = "~/Views/Shared/_SaleLayout.cshtml";
}

<div class="row top-head">
    <div class="col-md-12"><p><b><a style="color:#fff;" href="~/Dashboard/SaleDashboard">Sale</a> >> List Sale Order Setting </b></p></div>
</div>
<section>
    <form action="#" id="frmListSOSetting" class="form-horizontal as" enctype="multipart/form-data" method="post" accept-charset="utf-8">
        <div class="row inner">
            <div class="message"> 
            </div>   
            <div class="col-md-12 ftop"> 
            </div> 
            <div class="row col-sm-6 col-lg-12 col-md-12">
                <div class="col-md-3"></div>
                <div class="col-md-6">
                    <center>
                     <!-- <input class="btn btn-success" value="Search" alt="Save" id="btnSearch" onclick="SearchQuotationSetting()" type="button" border="0">&nbsp;&nbsp;
                        <input class="btn btn-success" type="button" id="btnReset" onclick="ClearFields()" value="Reset" /> &nbsp;&nbsp; -->  
                        <a class="btn btn-success" id="btnList" href="@Url.Content("~/SOSettings/AddEditSOSetting")">Add More Sale order Setting</a>
                    </center>
                </div>
                <div class="col-md-3"></div>
            </div>
            <div class="row col-sm-6 col-lg-12 col-md-12">
                <div id="divList"> 
                </div>
            </div>
        </div>
        <!-- Modal -->
        <div class="modal fade" id="alertModel" role="dialog">
            <div class="modal-dialog  modal-sm">
                <!-- Modal content-->
                <div class="modal-content">
                    <div class="modal-header">
                        <button type="button" class="close" data-dismiss="modal">&times;</button>
                        <h4 class="modal-title" id="modelHeader"></h4>
                    </div>
                    <div class="modal-body">
                        <p id="modelText"></p>
                    </div>
                    <div class="modal-footer">
                        <button type="button" class="btn btn-primary  btn-block" data-dismiss="modal">Close</button>
                    </div>
                </div>

            </div>
        </div>
    </form>
</section>
<script src="~/Scripts/SOSettings/ListSOSetting.js"></script>
<script src="~/Scripts/jquery-ui.min.js"></script>
<script src="~/Scripts/paging.js"></script>




