@{
    ViewBag.Title = "List Packing Material BOM List";
    Layout = "~/Views/Shared/_StoreLayout.cshtml";
}
<style>
    .table {
    width: 150% !important;
    max-width: 150% !important;
    margin-bottom: 20px;
}  
</style>
<link href="~/Content/jquery-ui.css" rel="stylesheet" />
<div class="row top-head">
    <div class="col-md-12"><p><b><a style="color:#fff;" href="~/Dashboard/StoreDashboard">Production </a> >>List Packing Material BOM </b></p></div>
</div>
<section>
    <form action="#" id="frmAddEditPMB" class="form-horizontal as" enctype="multipart/form-data" method="post" accept-charset="utf-8">
        <div class="row inner">
            <div class="message">

            </div>
            <div class="col-md-12 ftop">
                <div class="form-group col-md-4">
                    <label for="txtPMBNo" class="col-sm-4 control-label">Packing Material BOM No.</label>
                    <div class="col-sm-8 ">
                        <input class="form-control" id="txtPMBNo" name="txtPMBNo" tabindex="1" placeholder="Search with Packing Material BOM No." type="text" maxlength="50" />
                       
                    </div>
                </div>
                <div class="form-group  col-md-4">
                    <label for="ddlPackingListType" class="col-sm-4 control-label">Packing List Type</label>
                    <div class="col-sm-8">
                        <select id="ddlPackingListType" name="ddlPackingListType" tabindex="2" class="form-control"></select>
                    </div>
                </div>
                <div class="form-group  col-md-4">
                    <label for="ddlProductSubGroup" class="col-sm-4 control-label">Product Sub Group</label>
                    <div class="col-sm-8">
                        <select id="ddlProductSubGroup" name="ddlProductSubGroup" tabindex="2" class="form-control"></select>
                    </div>
                </div>
            </div>
            <br /><br />
            <div class="col-md-12" id="divDate">
                <div class="form-group col-md-4" style="display:none;">
                    <label for="ddlCompanyBranch" class="col-sm-4 control-label">Company Branch</label>
                    <div class="col-sm-8">
                        <select id="ddlCompanyBranch" name="ddlCompanyBranch" tabindex="12" class="form-control"></select>
                        <input id="hdnSessionCompanyBranchId" type="hidden" value="@ViewData["CompanyBranchId"]" />
                        <input id="hdnSessionUserID" type="hidden" value="@ViewData["UserId"]" /> 
                    </div>
                </div>
                <div class="form-group col-md-4">
                    <label for="txtFromDate" class="col-sm-4 control-label">From Date</label>
                    <div class="col-sm-8 ">
                        <input class="form-control" id="txtFromDate" name="txtFromDate" tabindex="3" value="@ViewData["fromDate"]" type="text" maxlength="50" />
                        <input id="hdnFromDate" name="hdnFromDate" value="@ViewData["fromDate"]" type="hidden" />
                    </div>
                </div>
                <div class="form-group col-md-4">
                    <label for="txtToDate" class="col-sm-4 control-label">To Date</label>
                    <div class="col-sm-8 ">
                        <input class="form-control" id="txtToDate" name="txtToDate" tabindex="4" value="@ViewData["toDate"]" type="text" maxlength="50" />
                        <input id="hdnToDate" name="hdnToDate" value="@ViewData["toDate"]" type="hidden" />
                    </div>
                </div>
            </div>
            
            <div class="row col-sm-12 col-lg-12 col-md-12">
                <div class="col-md-3"></div>
                <div class="col-md-6">
                    <center>
                        <input class="btn btn-success" value="Search" alt="Save" id="btnSearch" tabindex="5" onclick="SearchPackingMaterialBOM()" type="button" border="0">
                        <input class="btn btn-success" type="button" id="btnReset" tabindex="6" onclick="ClearFields()" value="Reset" /> 
                        <a class="btn btn-success" id="btnList" tabindex="7" href="@Url.Content("~/PackingMaterialBOM/AddEditPackingMaterialBOM?accessMode=1")">Add More Packing Material BOM</a>
                        <input class="btn btn-success" value="Export" alt="Export" tabindex="8" id="btnExport" onclick="Export()" type="button" border="0">
                    </center>
                </div>
            </div>
            <div class="row col-sm-6 col-lg-12 col-md-12">
                <br />
                <div id="divList">

                </div>
            </div>
        </div>
        <!-- Modal -->
        <div class="modal fade" id="alertModel" role="dialog">
            <div class="modal-dialog  modal-sm">
                <!-- Modal content-->
                <div class="modal-content">
                    <div class="modal-header">
                        <button type="button" class="close" data-dismiss="modal">&times;</button>
                        <h4 class="modal-title" id="modelHeader"></h4>
                    </div>
                    <div class="modal-body">
                        <p id="modelText"></p>
                    </div>
                    <div class="modal-footer">
                        <button type="button" class="btn btn-primary  btn-block" data-dismiss="modal">Close</button>
                    </div>
                </div>

            </div>
        </div>
    </form>
</section>
<script src="~/Scripts/PackingMaterialBOM/ListPackingMaterialBOM.js"></script>
<script src="~/Scripts/jquery-ui.min.js"></script>
<script src="~/Scripts/paging.js"></script>
<script src="~/Scripts/Common.js"></script>