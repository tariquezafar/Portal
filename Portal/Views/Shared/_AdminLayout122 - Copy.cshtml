            var sidebarid = document.getElementById("hdnSidebar").value;
            if (sidebarid == 0) {
                document.getElementById("left-menu").className = "wrapper-sidebar";
                document.getElementById("page-content-wrapper").className = "wrapper-padding";
                document.getElementById("left-menu").className = "sidebar-lefts";
                document.getElementById("left-menu").className = "sidebars-margin";
                document.getElementById("hdnSidebar").value = 1;
            }
            else
            {
                document.getElementById("left-menu").remove("wrapper-sidebar");
                document.getElementById("left-menu").classList.remove("wrapper-sidebar");
                document.getElementById("page-content-wrapper").classList.remove("wrapper-padding");
                document.getElementById("left-menu").classList.remove("sidebar-lefts");
                document.getElementById("left-menu").classList.remove("sidebars-margin");
                document.getElementById("hdnSidebar").value = 0;
            }
