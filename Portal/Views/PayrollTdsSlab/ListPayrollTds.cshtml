@{
    ViewBag.Title = "List Payroll Tds";
    Layout = "~/Views/Shared/_PayrollLayout.cshtml";
}


<div class="row top-head">
    <div class="col-md-12"><p><b><a style="color:#fff;" href="~/Dashboard/Payroll">Payroll</a> >> List Payroll TDS </b></p></div>
</div>
<section>
    <form action="#" id="frmListProductMainGroup" class="form-horizontal as" enctype="multipart/form-data" method="post" accept-charset="utf-8">
        <div class="row inner">
            <div class="message">

            </div>
            <div class="col-md-12 ftop">
                <div class="form-group col-md-4">
                    <label for="txtFromDate" class="col-sm-4 control-label">From Date</label>
                    <div class="col-sm-8">
                        <input class="form-control" id="txtFromDate" name="txtFromDate" tabindex="1" placeholder="Enter From Date" type="text" maxlength="100" value="@ViewData["fromDate"]"/>
                        <input id="hdnFromDate" name="hdnFromDate" value="@ViewData["fromDate"]" type="hidden" />

                    </div>
                </div>
                <div class="form-group col-md-4">
                    <label for="txtToDate" class="col-sm-4 control-label">To Date</label>
                    <div class="col-sm-8">
                        <input class="form-control" id="txtToDate" name="txtToDate" tabindex="2" placeholder="Enter To Date" type="text" maxlength="100" value="@ViewData["toDate"]"/>
                        <input id="hdnToDate" name="hdnToDate" value="@ViewData["toDate"]" type="hidden" />
                    </div>
                </div>
                <div class="form-group col-md-4">
                    <label for="ddlCategory" class="col-sm-4 control-label">Select Category</label>
                    <div class="col-sm-8">
                        <select id="ddlCategory" name="ddlCategory" tabindex="3" class="form-control">
                            <option value="0">-Select Category-</option>
                            <option value="1">Mrs</option>
                            <option value="2">Miss</option>
                            <option value="3">F Senior Citizen</option>
                            <option value="4">M Senior Citizen</option>
                        </select>

                    </div>
                </div>
            </div>

           
            <div class="row col-sm-6 col-lg-12 col-md-12">
                <div class="col-md-3"></div>
                <div class="col-md-6">
                    <center>
                        <input class="btn btn-success" value="Search" alt="Save" id="btnSearch" tabindex="4" onclick="SearchPayrollTds()" type="button" border="0">
                        <input class="btn btn-success" value="Reset"  type="button" id="btnReset" tabindex="5" onclick="ClearFields()" />
                        <a class="btn btn-success" id="btnList" tabindex="6" href="@Url.Content("~/PayrollTdsSlab/AddEditPayrollTdsSlab?accessMode=1")">Add Payroll TDS</a>
                    </center>
                </div>
                <div class="col-md-3"></div>
            </div>
            <div class="row col-sm-6 col-lg-12 col-md-12">
                <div id="divList" style="margin:50px;margin-top:10px">

                </div>
            </div>
        </div>
        <!-- Modal -->
        <div class="modal fade" id="alertModel" role="dialog">
            <div class="modal-dialog  modal-sm">
                <!-- Modal content-->
                <div class="modal-content">
                    <div class="modal-header">
                        <button type="button" class="close" data-dismiss="modal">&times;</button>
                        <h4 class="modal-title" id="modelHeader"></h4>
                    </div>
                    <div class="modal-body">
                        <p id="modelText"></p>
                    </div>
                    <div class="modal-footer">
                        <button type="button" class="btn btn-primary  btn-block" data-dismiss="modal">Close</button>
                    </div>
                </div>

            </div>
        </div>
    </form>
</section>
<script src="~/Scripts/jquery-ui.js"></script>
<script src="~/Scripts/jquery.tablesorter.js"></script>
<script src="~/Scripts/lightbox.js"></script>
<script src="~/Scripts/PayrollTds/ListPayrollTds.js"></script>