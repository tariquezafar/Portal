@{
    ViewBag.Title = "List GLBalanceTransfer";
    Layout = "~/Views/Shared/_AccountLayout.cshtml";
}

<div class="row top-head">
    <div class="col-md-12"><p><b><a style="color:#fff;" href="~/Dashboard/AccountDashboard">Account</a> >> List GL Balance Transfer </b></p></div>
</div>
<section>
    <form action="#" id="frmAddEditProductSubGroup" class="form-horizontal as" enctype="multipart/form-data" method="post" accept-charset="utf-8">
        <div class="row inner">
            <div class="message">

            </div>
            <div class="col-md-12 ftop">
                <div class="form-group col-md-4">
                    <label for="txtFromFinYearID" class="col-sm-4 control-label">From FinYear</label>
                    <div class="col-sm-8">
                        <input class="form-control numeric-only" id="txtFromFinYearID" name="txtFromFinYearID" tabindex="1" placeholder="Search From FinYear." type="text" maxlength="5" />                      
                    </div>
                </div>
                <div class="form-group col-md-4">
                    <label for="txtToFinYearID" class="col-sm-4 control-label">To FinYear</label>
                    <div class="col-sm-8">
                        <input class="form-control numeric-only" id="txtToFinYearID" name="txtToFinYearID" tabindex="1" placeholder="Search To FinYear." type="text" maxlength="5" />                      
                    </div>
                </div> 
                <div class="form-group col-md-4">
                    <label for="txtCreatedByUserName" class="col-sm-4 control-label">Created By UserName</label>
                    <div class="col-sm-8">
                        <input class="form-control" id="txtCreatedByUserName" name="txtCreatedByUserName" tabindex="3" placeholder="Search User Name" type="text" maxlength="100" />
                    </div>
                </div>
            </div> 
            <div class="col-md-12">
                
                <div class="form-group col-md-4">
                    <label for="ddlCompanyBranch" class="col-sm-4 control-label">Company Branch</label>
                    <div class="col-sm-8">
                        <select id="ddlCompanyBranch" name="ddlCompanyBranch" tabindex="10" class="form-control"></select>
                        <input id="hdnSessionCompanyBranchId" type="hidden" value="@ViewData["CompanyBranchId"]" />
                        <input id="hdnSessionUserID" type="hidden" value="@ViewData["UserId"]" />
                    </div>
                </div>
              
            </div>
             
            <div class="row col-sm-6 col-lg-12 col-md-12">
                <div class="col-md-3"></div>
                <div class="col-md-6">
                    <center>
                        <input class="btn btn-success" value="Search" alt="Save" id="btnSearch" tabindex="5" onclick="SearchGLTransfer()" type="button" border="0">
                        <input class="btn btn-success" type="button" id="btnReset" tabindex="6" onclick="ClearFields()" value="Reset" />
                        <a class="btn btn-success" id="btnList" tabindex="7" href="@Url.Content("~/GLBalanceTransfer/GLBalanceTransfer?accessMode=1")">Add Tranf GL closing Bal</a>                       
                    </center>
                </div>
                <div class="col-md-3"></div>
            </div>
            <div class="row col-sm-6 col-lg-12 col-md-12">
                <div id="divList" style="margin:50px;margin-top:10px">

                </div>
            </div>
        </div>
        <!-- Modal -->
        <div class="modal fade" id="printModel" role="dialog">
            <div class="modal-dialog  modal-sm">
                <!-- Modal content-->
                <div class="modal-content">
                    <div class="modal-header">
                        <button type="button" class="close" data-dismiss="modal">&times;</button>
                        <h4 class="modal-title">Chasis Model Print</h4>
                    </div>
                    <div class="modal-body">
                        <div class="form-group">
                            <label for="ddlPrintOption" class="col-sm-4 control-label">Report Option</label>
                            <div class="col-sm-8">
                                <select id="ddlPrintOption" name="ddlPrintOption" onchange="ShowHidePrintOption()" tabindex="1" class="form-control">
                                    <option selected="selected" value="PDF">PDF</option>
                                    <option value="Excel">Excel</option>
                                </select>
                            </div>
                        </div>
                    </div>
                    <div class="modal-footer">
                        <a class="btn btn-success" target="_blank" href="#" id="btnPdf">Print</a>
                        <a class="btn btn-success" style="display:none;" target="_blank" href="#" id="btnExcel">Print</a>
                        <button type="button" class="btn btn-primary" data-dismiss="modal">Close</button>

                    </div>
                </div>

            </div>
        </div>
        <div class="modal fade" id="alertModel" role="dialog">
            <div class="modal-dialog  modal-sm">
                <!-- Modal content-->
                <div class="modal-content">
                    <div class="modal-header">
                        <button type="button" class="close" data-dismiss="modal">&times;</button>
                        <h4 class="modal-title" id="modelHeader"></h4>
                    </div>
                    <div class="modal-body">
                        <p id="modelText"></p>
                    </div>
                    <div class="modal-footer">
                        <button type="button" class="btn btn-primary  btn-block" data-dismiss="modal">Close</button>
                    </div>
                </div>

            </div>
        </div>
    </form>
</section>
<script src="~/Scripts/GLBalanceTransfer/LIstGLBalanceTransfer.js"></script>
<script src="~/Scripts/jquery-ui.min.js"></script>
<script src="~/Scripts/paging.js"></script>