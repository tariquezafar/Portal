@{
    ViewBag.Title = "List Payroll Process Period";
    Layout = "~/Views/Shared/_PayrollLayout.cshtml";
}
<link href="~/Content/jquery-ui.css" rel="stylesheet" />
<div class="row top-head">
    <div class="col-md-12"><p><b><a style="color:#fff;" href="~/Dashboard/PayrollDashboard">Payroll </a> >> List Payroll Process Period </b></p></div>
</div>
<section>
    <form action="#" id="frmAddEditPO" class="form-horizontal as" enctype="multipart/form-data" method="post" accept-charset="utf-8">
        <div class="row inner">
            <div class="message">

            </div>
            <div class="col-md-12 ftop">
                <div class="form-group col-md-4">
                    <label for="ddlMonth" class="col-sm-4 control-label">Payroll Process Month</label>
                    <div class="col-sm-8 ">
                        <select id="ddlMonth" name="ddlMonth"  tabindex="1" class="form-control"></select>
                    </div>
                </div>
                <div class="form-group  col-md-4">
                    <label for="ddlPayrollProcessStatus" class="col-sm-4 control-label">Payroll Status</label>
                    <div class="col-sm-8">
                        <select id="ddlPayrollProcessStatus" tabindex="2" name="ddlPayrollProcessStatus" class="form-control">
                            <option value="">-Select-</option>
                            <option value="In Process">In Process</option>
                            <option value="Final">Final</option>
                        </select>
                    </div>
                </div>
                <div class="form-group col-md-4">
                    <label for="ddlPayrollLocked" class="col-sm-4 control-label">Payroll Locked Status</label>
                    <div class="col-sm-8 ">
                        <select id="ddlPayrollLocked" name="ddlPayrollLocked" tabindex="3" class="form-control">
                            <option value="">-Select-</option>
                            <option value="0">Unlocked</option>
                            <option value="1">Locked</option>
                        </select>
                    </div>
                </div>

            </div>

            <div class="col-md-12">
                <div class="form-group col-md-4">
                    <label for="ddlCompanyBranch" class="col-sm-4 control-label">Company Branch</label>
                    <div class="col-sm-8">
                        <select id="ddlCompanyBranch" name="ddlCompanyBranch" tabindex="46" onchange="GetBranchStateId()" class="form-control"></select>
                        <input id="hdnBillingStateId" type="hidden" value="0" />
                        <input id="hdnSessionCompanyBranchId" type="hidden" value="@ViewData["CompanyBranchId"]" />
                        <input id="hdnSessionUserID" type="hidden" value="@ViewData["UserId"]" />
                    </div>
                </div>
            </div>


                <div class="row col-sm-12 col-lg-12 col-md-12">
                    <div class="col-md-3"></div>
                    <div class="col-md-6">
                        <center>
                            <input class="btn btn-success" value="Search" alt="Search" tabindex="4" id="btnSearch" onclick="SearchPayrollProcessPeriod()" type="button" border="0">
                            <input class="btn btn-success" type="button" tabindex="5" id="btnReset" onclick="ClearFields()" value="Reset" />
                            <a class="btn btn-success" id="btnList" tabindex="6" href="@Url.Content("~/PayrollProcessPeriod/AddEditPayrollProcessPeriod?accessMode=1")">Add More Payroll Process Period</a>
                        </center>
                    </div>
                    <div class="col-md-3"></div>
                </div>
                <div class="row col-sm-6 col-lg-12 col-md-12">
                    <div id="divList">

                    </div>
                </div>
            </div>
        <!-- Modal -->
        <div class="modal fade" id="alertModel" role="dialog">
            <div class="modal-dialog  modal-sm">
                <!-- Modal content-->
                <div class="modal-content">
                    <div class="modal-header">
                        <button type="button" class="close" data-dismiss="modal">&times;</button>
                        <h4 class="modal-title" id="modelHeader"></h4>
                    </div>
                    <div class="modal-body">
                        <p id="modelText"></p>
                    </div>
                    <div class="modal-footer">
                        <button type="button" class="btn btn-primary  btn-block" data-dismiss="modal">Close</button>
                    </div>
                </div>

            </div>
        </div>
    </form>
</section>

<script src="~/Scripts/jquery-ui.min.js"></script>
<script src="~/Scripts/paging.js"></script>
<script src="~/Scripts/PayrollProcessPeriod/ListPayrollProcessPeriod.js"></script>