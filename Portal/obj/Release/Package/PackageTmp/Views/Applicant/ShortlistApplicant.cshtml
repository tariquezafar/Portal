                            <label for="ddlDesignation" class="col-sm-4 control-label">Position Applied&nbsp;<span class="Mandatory">(*)</span></label>
                            <div class="col-sm-8">
                                <select id="ddlDesignation" name="ddlDesignation" tabindex="16" class="form-control"></select>
                            </div>
                        </div>
                    </div>


