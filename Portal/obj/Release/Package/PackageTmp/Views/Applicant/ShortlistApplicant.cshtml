                                <input class="form-control" id="txtApplicantNo" name="txtApplicantNo" tabindex="1" placeholder="Auto Generated Applicant No." type="text" maxlength="100" />
                                <input id="hdnApplicantId" type="hidden" value="@ViewData["applicantId"]" />
                                <input id="hdnAccessMode" type="hidden" value="@ViewData["accessMode"]" />
                            </div>
                        </div>
                        <div class="form-group col-md-6">
                            <label for="txtApplicationDate" class="col-sm-4 control-label right-text">Date&nbsp;<span class="Mandatory">(*)</span></label>
                            <div class="col-sm-8">
                                <input class="form-control" id="txtApplicationDate" name="txtApplicationDate" tabindex="2" placeholder="Select Application Date" value="@ViewData["currentDate"]" type="text" maxlength="20" />
                                <input id="hdnApplicationDate" name="hdnApplicationDate" value="@ViewData["currentDate"]" type="hidden" />
                            </div>
                        </div>
                    </div>
                    <div class="col-md-12">
