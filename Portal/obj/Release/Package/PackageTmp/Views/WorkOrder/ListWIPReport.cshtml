@{
    ViewBag.Title = "List Product BOM";
    Layout = "~/Views/Shared/_StoreLayout.cshtml";
}
<link href="~/Content/jquery-ui.css" rel="stylesheet" />
<style>
      .ui-autocomplete {
         max-height: 500px;
         overflow-y: auto;
         z-index:999999;        
         overflow-x: hidden;
     }
     /* IE 6 doesn't support max-height
    * we use height instead, but this forces the menu to always be this tall
    */
     * html .ui-autocomplete {
         height: 500px;
     }
</style>
<div class="row top-head">
    <div class="col-md-12"><p><b><a style="color:#fff;" href="~/Dashboard/StoreDashboard">Production</a> >>WIP Report</b></p></div>
</div>
<section>
    <form action="#" id="frmListProductBOM" class="form-horizontal as" enctype="multipart/form-data" method="post" accept-charset="utf-8">
        <div class="row inner">
            <div class="message">

            </div>
            <div class="col-md-12" style="display:none;">
                <div class="form-group col-md-6">
                    <label for="ddlAssemblyType" class="col-sm-4 control-label">Assembly Type</label>
                    <div class="col-sm-8">
                        <select id="ddlAssemblyType" name="ddlAssemblyType" tabindex="2" onchange="GenerateReportParameters();" class="form-control">
                            <option selected="selected" value="0">-Select Assembly Type-</option>
                            <option value="MA">Main Assembly</option>
                            <option value="SA">Sub Assembly</option>
                        </select>
                    </div>
                </div>
            </div>
            <div class="col-md-12 ">
                
                <div class="form-group col-md-6">

                    <label for="txtWorkOrderNo" class="col-sm-4 control-label">Work Order No.&nbsp;</label>
                    <div class="col-sm-8">
                        <input class="form-control" id="txtWorkOrderNo" name="txtWorkOrderNo" tabindex="2" placeholder="Select  Work Order No." type="text" maxlength="100" />
                        <input id="hdnWorkOrderID" type="hidden" value="0" />

                    </div>
                </div>

            </div>
            
             <div class="row col-sm-6 col-lg-12 col-md-12">
                <div class="col-md-3"></div>
                <div class="col-md-6">
                    <center>                      
                        <a class="btn btn-success" target="_blank"  href="#"  alt="Export" tabindex="10" id="btnExport" border="0">Generate WIP Report</a>                   
                    </center>
                </div>
                <div class="col-md-3"></div>
            </div>
          
        </div>    
        
        
    </form>
</section>
<script src="~/Scripts/WorkOrder/ListWIPReport.js"></script>
<script src="~/Scripts/jquery-ui.min.js"></script>
<script src="~/Scripts/paging.js"></script>