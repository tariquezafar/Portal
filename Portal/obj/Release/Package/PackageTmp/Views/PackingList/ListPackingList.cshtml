@{
    ViewBag.Title = "List PackingList";
    Layout = "~/Views/Shared/_SaleLayout.cshtml";
}
<style>
    .table {
    width: 150% !important;
    max-width: 150% !important;
    margin-bottom: 20px;
}
</style>
<link href="~/Content/jquery-ui.css" rel="stylesheet" />
<div class="row top-head">
    <div class="col-md-12"><p><b><a style="color:#fff;" href="~/Dashboard/SaleDashboard">Sale </a> >> List PackingList </b></p></div>
</div>
<section>
    <form action="#" id="frmPackingList" class="form-horizontal as" enctype="multipart/form-data" method="post" accept-charset="utf-8">
        <div class="row inner">
            <div class="message">

            </div>
            <div class="col-md-12 ftop">

                <div class="form-group col-md-4">
                    <label for="txtPackingListName" class="col-sm-4 control-label">Packing List Name</label>
                    <div class="col-sm-8 ">
                        <input class="form-control" id="txtPackingListName" name="txtPackingListName" tabindex="1" placeholder="Search with PackingList Name" type="text" maxlength="50" />
                      
                     </div>
                </div>

                <div class="form-group col-md-4">
                    <label for="txtPackingListStatus" class="col-sm-4 control-label">Packing List Status</label>
                    
                        @*<input class="form-control" id="txtPackingListStatus" name="txtPackingListStatus" tabindex="2" placeholder="Search with Packing List Status" type="text" maxlength="50" />*@
                    
                        <div class="col-sm-8">
                            <select id="ddlPackingListStatus" name="ddlPackingListStatus" tabindex="2" class="form-control">
                                <option value="0">-Packing List Status-</option>
                                <option value="Draft">Draft</option>
                                <option value="Final">Final</option>
                            </select>
                      
                    </div>
                </div>
                <div class="form-group col-md-4">
                    <label for="ddlCompanyBranch" class="col-sm-4 control-label">Company Branch</label>
                    <div class="col-sm-8">
                        <select id="ddlCompanyBranch" name="ddlCompanyBranch" tabindex="5" class="form-control"></select>
                        <input id="hdnSessionCompanyBranchId" type="hidden" value="@ViewData["CompanyBranchId"]" />
                        <input id="hdnSessionUserID" type="hidden" value="@ViewData["UserId"]" />

                    </div>
                </div>
            </div>


                <div class="row col-sm-12 col-lg-12 col-md-12">
                    <div class="col-md-3"></div>
                    <div class="col-md-6">
                        <center>
                            <input class="btn btn-success" value="Search" alt="Save" id="btnSearch" tabindex="7" onclick="SearchPackingList()" type="button" border="0">
                            <input class="btn btn-success" type="button" id="btnReset" tabindex="8" onclick="ClearFields()" value="Reset" />
                            <a class="btn btn-success" id="btnList" tabindex="9" href="@Url.Content("~/PackingList/AddEditPackingList?accessMode=1")">Add More Packing List</a>
                        </center>
                    </div>
                </div>
                <div class="row col-sm-6 col-lg-12 col-md-12">
                    <div id="divList">

                    </div>
                </div>
            </div>
        <!-- Modal -->
        <div class="modal fade" id="alertModel" role="dialog">
            <div class="modal-dialog  modal-sm">
                <!-- Modal content-->
                <div class="modal-content">
                    <div class="modal-header">
                        <button type="button" class="close" data-dismiss="modal">&times;</button>
                        <h4 class="modal-title" id="modelHeader"></h4>
                    </div>
                    <div class="modal-body">
                        <p id="modelText"></p>
                    </div>
                    <div class="modal-footer">
                        <button type="button" class="btn btn-primary  btn-block" data-dismiss="modal">Close</button>
                    </div>
                </div>

            </div>
        </div>
    </form>
</section>
<script src="~/Scripts/PackingList/ListPackingList.js"></script>
<script src="~/Scripts/jquery-ui.min.js"></script>
<script src="~/Scripts/paging.js"></script>
<script src="~/Scripts/Common.js"></script>