        <!-- circle css-- >
        /**
        * Position icons into circle (SO)
        * http://stackoverflow.com/q/12813573/1397351
        */
        .circle-container {
            position: relative;
            width: 24em;
            height: 24em;
            padding: 2.8em; /*= 2em * 1.4 (2em = half the width of an img, 1.4 = sqrt(2))*/
            border: dashed 1px;
            border-radius: 50%;
            margin: 1.75em auto 0;
        }

        .circle-container a {
            display: block;
            position: absolute;
            top: 41%; 
            left: 43%;
            <!-- width: 9em; -->
            margin: -2em; /* 2em = 4em/2 */ /* half the width */
        }

        .circle-container img {
            display: block;
        }

        .deg0 { -webkit-transform:translate(11em) rotate(60deg) rotate(-61deg);
    margin-top: 37px; } /* 12em = half the width of the wrapper */
	.deg25 { -webkit-transform:rotate(93deg) translate(12em) rotate(-93deg); }
.deg45 { -webkit-transform:rotate(55deg) translate(12em) rotate(-59deg); }
.deg135 { -webkit-transform:rotate(157deg) translate(15em) rotate(-155deg); }
.deg180 { -webkit-transform:rotate(8deg) translate(-15em) rotate(-8deg); }
.deg225 { -webkit-transform:rotate(218deg) translate(14em) rotate(-219deg); }
.deg315 { -webkit-transform:rotate(332deg) translate(12em) rotate(-331deg); }
 .deg425 {
    -webkit-transform:rotate(310deg) translate(-15em) rotate(-308deg); }
.deg515{ -webkit-transform:rotate(211deg) translate(2em) rotate(150deg);
    margin-top: -20%; }
.deg610{ -webkit-transform:rotate(150deg) translate(12em) rotate(209deg); }
.deg290{ -webkit-transform:rotate(295deg) translate(11em) rotate(70deg); }
        /* this is just for showing the angle on hover */
        .circle-container a:not(.center):before {
            position: absolute;
            width: 4em;
            color: white;
            opacity: 0;
            background: rgba(0,0,0,.5);
            font: 1.25em/3.45 Courier, monospace;
            letter-spacing: 2px;
            text-decoration: none;
            text-indent: -2em;
            text-shadow: 0 0 .1em deeppink;
            transition: .7s; /* only works in Firefox */
            content: attr(class)'°';
            display: none;
        }

        .circle-container a:hover:before {
            opacity: 1;
        }

        /* this is for showing the circle on which the images are placed */
        .circle-container:after {
            position: absolute;
            top: 2.8em;
            left: 2.8em;
            width: 24em;
            height: 24em;
            border: dashed 1px deeppink;
            border-radius: 50%;
            opacity: 0;
            pointer-events: none;
            content: '';
        }

        .circle-container:hover:after {
            opacity: 0;
        }

        .circle-container a:not(.center):after {
            position: absolute;
            top: 50%;
            left: 50%;
            width: 4px;
            height: 4px;
            border-radius: 50%;
            box-shadow: 0 0 .5em .5em white;
            margin: -2px;
            background: deeppink;
            opacity: .3;
            content: '';
        }

        .circle-container:hover a:after {
            opacity: 1;
        }

        .circle-container a:hover:after {
            opacity: .3;
        }

