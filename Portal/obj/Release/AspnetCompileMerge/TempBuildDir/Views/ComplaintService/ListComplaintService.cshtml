@{
    ViewBag.Title = "ListComplaintService";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}
<link href="~/Content/jquery-ui.css" rel="stylesheet" />

<div class="row top-head">
    <div class="col-md-12"><p><b><a style="color:#fff;" href="~/Dashboard/AdminDashboard">Admin</a> >> List Complaint Service </b></p></div>
</div>
<section>
    <form action="#" id="frmAddEditComplaintService" class="form-horizontal as" enctype="multipart/form-data" method="post" accept-charset="utf-8">
        <div class="row inner">
            <div class="message">

            </div>
            <div class="col-md-12 ftop">
                <div class="form-group  col-md-4">
                    <label for="txtComplaintNo" class="col-sm-4 control-label">Complaint No.</label>
                    <div class="col-sm-8">
                        <input class="form-control" id="txtComplaintNo" name="txtComplaintNo" tabindex="1" readonly="readonly" placeholder="Search with Complaint No." type="text" maxlength="100" />
                       
                    </div>
                </div>
                <div class="form-group col-md-4">
                    <label for="ddlComplaintType" class="col-sm-4 control-label">Complaint Type&nbsp;</label>
                    <div class="col-sm-8">
                        @*<select id="ddlComplaintType" name="ddlComplaintType" tabindex="3" onchange="GetBranchStateId()" class="form-control"></select>*@
                        <select id="ddlComplaintType" name="ddlComplaintType" tabindex="2" class="form-control">
                            <option selected="selected" value="0">-Select Complaint Type-</option>
                            <option value="Complaint">Complaint</option>
                            <option value="Suggestion">Suggestion</option>
                            <option value="Feedback">Feedback</option>
                        </select>
                    </div>
                </div>
                <div class="form-group col-md-4">
                    <label for="ddlComplaintMode" class="col-sm-4 control-label">Complaint Mode&nbsp;</label>
                    <div class="col-sm-8">
                        <select id="ddlComplaintMode" name="ddlComplaintMode" tabindex="3" class="form-control">
                            <option selected="selected" value="0">-Select Complaint Mode-</option>
                            <option value="Telephonic">Telephonic</option>
                            <option value="Email">Email</option>
                        </select>
                    </div>
                </div>
            </div>
            <div class="col-md-12">
                <div class="form-group col-md-4">
                    <label for="txtCustomerMobile" class="col-sm-4 control-label">Customer Mobile&nbsp;</label>
                    <div class="col-sm-8">
                        <input class="form-control input-number numeric-only" id="txtCustomerMobile" name="txtCustomerMobile" tabindex="4" placeholder="Enter Customer Mobile" type="text" maxlength="10" />

                    </div>
                </div>
                <div class="form-group col-md-4">
                        <label for="txtCustomerName" class="col-sm-4 control-label">Customer Name&nbsp;</label>
                        <div class="col-sm-8 ">
                            <input class="form-control" id="txtCustomerName" name="txtCustomerName" tabindex="5" placeholder="Customer Name" type="text" maxlength="50" />
                        </div>
                    </div>
                <div class="form-group  col-md-4">
                        <label for="ddlStatus" class="col-sm-4 control-label">Status</label>
                        <div class="col-sm-8">
                            <select name="ddlStatus" id="ddlStatus" tabindex="6" class="form-control">
                                <option value="">All</option>
                                <option value="1">Active</option>
                                <option value="0">InActive</option>
                            </select>
                        </div>
                    </div>
                   
                </div>
            <div class="col-md-12">
                <div class="form-group col-md-4">
                    <label for="ddlCompanyBranch" class="col-sm-4 control-label">Company Branch</label>
                    <div class="col-sm-8">
                        <select id="ddlCompanyBranch" name="ddlCompanyBranch" tabindex="5" class="form-control"></select>
                        <input id="hdnSessionCompanyBranchId" type="hidden" value="@ViewData["CompanyBranchId"]" />
                        <input id="hdnSessionUserID" type="hidden" value="@ViewData["UserId"]" />

                    </div>
                </div>
            </div>

                <div class="row col-sm-12 col-lg-12 col-md-12">
                    <div class="col-md-3"></div>
                    <div class="col-md-6">
                        <center>
                            <input class="btn btn-success" value="Search" alt="Save" tabindex="7" id="btnSearch" onclick="SearchComplaintservice()" type="button" border="0">&nbsp;&nbsp;
                            <input class="btn btn-success" type="button" id="btnReset" tabindex="8" onclick="ClearFields()" value="Reset" /> &nbsp;&nbsp;
                            <a class="btn btn-success" id="btnList" tabindex="9" href="@Url.Content("~/ComplaintService/AddEditComplaintService?accessMode=1")">Add More Complaint Service</a>
                        </center>
                    </div>
                    <div class="col-md-3"></div>
                </div>
                <div class="row col-sm-6 col-lg-12 col-md-12">
                    <div id="divList">

                    </div>
                </div>
            </div>
        
        <!-- Modal -->
        <div class="modal fade" id="alertModel" role="dialog">
            <div class="modal-dialog  modal-sm">
                <!-- Modal content-->
                <div class="modal-content">
                    <div class="modal-header">
                        <button type="button" class="close" data-dismiss="modal">&times;</button>
                        <h4 class="modal-title" id="modelHeader"></h4>
                    </div>
                    <div class="modal-body">
                        <p id="modelText"></p>
                    </div>
                    <div class="modal-footer">
                        <button type="button" class="btn btn-primary  btn-block" data-dismiss="modal">Close</button>
                    </div>
                </div>

            </div>
        </div>
    </form>
</section>

<script src="~/Scripts/ComplaintService/ListComplaintService.js"></script>
<script src="~/Scripts/jquery-ui.min.js"></script>
<script src="~/Scripts/paging.js"></script>
<script src="~/Scripts/Common.js"></script>
