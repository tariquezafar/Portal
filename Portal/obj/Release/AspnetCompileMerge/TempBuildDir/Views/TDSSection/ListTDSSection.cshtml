@{
    ViewBag.Title = "List TDS Section";
    Layout = "~/Views/Shared/_PayrollLayout.cshtml";
}

<div class="row top-head">
    <div class="col-md-12"><p><b><a style="color:#fff;" href="~/Dashboard/PayrollDashboard">Payroll</a> >>List TDS Section</b></p></div>
</div>
<section>
    <form action="#" id="frmAddEditCountry" class="form-horizontal as" enctype="multipart/form-data" method="post" accept-charset="utf-8">
        <div class="row inner">
            <div class="message">

            </div>
            <div class="col-md-12 ftop">
                <div class="form-group col-md-4">
                    <label for="txtSectionName" class="col-sm-4 control-label">Section Name</label>
                    <div class="col-sm-8">
                        <input class="form-control" id="txtSectionName" tabindex="1" name="txtSectionName" placeholder="Search with Section Name" type="text" maxlength="100" />
                    </div>
                </div>
                <div class="form-group col-md-4">
                    <label for="txtSectionDescription" class="col-sm-4 control-label">Section Description</label>
                    <div class="col-sm-8">
                        <input id="txtSectionDescription" name="txtSectionDescription"  tabindex="2" type="text" placeholder="Search with Section Desc" maxlength="100" class="form-control alpha-space-only" />
                    </div> 
                </div>
                <div class="form-group col-md-4">
                    <label for="txtSectionMAXValue" class="col-sm-4 control-label">Section MAX Value</label>
                    <div class="col-sm-8">
                        <input id="txtSectionMAXValue" name="txtSectionMAXValue" tabindex="3" type="text" placeholder="Search with txtSection MAX Value" maxlength="10" class="form-control numeric-only" />
                    </div>
                </div>
            </div>
            <div class="col-md-12">

                <div class="form-group col-md-4">
                    <label for="ddlCompanyBranch" class="col-sm-4 control-label">Company Branch</label>
                    <div class="col-sm-8">
                        <select id="ddlCompanyBranch" name="ddlCompanyBranch" tabindex="46" onchange="GetBranchStateId()" class="form-control"></select>
                        <input id="hdnBillingStateId" type="hidden" value="0" />
                        <input id="hdnSessionCompanyBranchId" type="hidden" value="@ViewData["CompanyBranchId"]" />
                        <input id="hdnSessionUserID" type="hidden" value="@ViewData["UserId"]" />
                    </div>
                </div>
                
                <div class="form-group col-md-4">
                    <label for="selstatus" class="col-sm-4 control-label" >Status</label>
                    <div class="col-sm-8">
                        <!--- <input id="chkStatus" type="checkbox" name="chkStatus" class="" /> --->
                        <select class="form-control" name="ddlStatus" tabindex="3" id="ddlStatus">
                            <option value="">All</option>
                            <option value="1">Active</option>
                            <option value="0">InActive</option>
                        </select>
                    </div>
                </div>
            </div>

            
            <div class="row col-sm-6 col-lg-12 col-md-12">
                <div class="col-md-3"></div>
                <div class="col-md-6">
                    <center>
                        <input class="btn btn-success" value="Search" alt="Save" id="btnSearch" tabindex="4" onclick="SearchTDSSection()" type="button" border="0">
                        <input class="btn btn-success" type="button" id="btnReset" tabindex="5" onclick="ClearFields()" value="Reset" /> 
                        <a class="btn btn-success" id="btnList" tabindex="6" href="@Url.Content("~/TDSSection/AddEditTDSSection?accessMode=1")">Add More TDS Section</a>
                    </center>
                </div>
                <div class="col-md-3"></div>
            </div>
            <div class="row col-sm-6 col-lg-12 col-md-12">
                <div id="divList" style="margin:100px;margin-top:10px">

                </div>
            </div>
         </div>
        <!-- Modal -->
        <div class="modal fade" id="alertModel" role="dialog">
            <div class="modal-dialog  modal-sm">
                <!-- Modal content-->
                <div class="modal-content">
                    <div class="modal-header">
                        <button type="button" class="close" data-dismiss="modal">&times;</button>
                        <h4 class="modal-title" id="modelHeader"></h4>
                    </div>
                    <div class="modal-body">
                        <p id="modelText"></p>
                    </div>
                    <div class="modal-footer">
                        <button type="button" class="btn btn-primary  btn-block" data-dismiss="modal">Close</button>
                    </div>
                </div>

            </div>
        </div>
    </form>
</section>
<script src="~/Scripts/TDSSection/ListTDSSection.js"></script>
<script src="~/Scripts/jquery-ui.min.js"></script>
<script src="~/Scripts/paging.js"></script>