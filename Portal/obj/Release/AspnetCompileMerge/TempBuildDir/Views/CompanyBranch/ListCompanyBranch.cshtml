@{
    ViewBag.Title = "List CompaniesBranch";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<div class="row top-head">
    <div class="col-md-12"><p><b><a style="color:#fff;" href="~/Dashboard/AdminDashboard">Admin</a> >> Add/Edit Company Branch </b></p></div>
</div>
<section>
    <form action="#" id="frmAddEditCompany" class="form-horizontal as" enctype="multipart/form-data" method="post" accept-charset="utf-8">
        <div class="row inner">
            <div class="message">

            </div>
            <div class="col-md-12 ftop">
                <div class="form-group col-md-4">
                    <label for="txtCompanyName" class="col-sm-4 control-label">Branch Name</label>
                    <div class="col-sm-8">
                        <input class="form-control" id="txtBBranchName" name="txtBBranchName" tabindex="1" placeholder="Search with Branch Name" type="text" maxlength="100" />
                    </div>
                </div>
                <div class="form-group col-md-4">
                    <label for="txtPhone" class="col-sm-4 control-label">Phone </label>
                    <div class="col-sm-8">
                        <input id="txtBPhone" name="txtBPhone" type="text" tabindex="2" placeholder="Search with Mobile Number" maxlength="20" class="form-control  input-number numeric-only" />
                    </div>
                </div>
                <div class="form-group col-md-4">
                    <label for="txtCity" class="col-sm-4 control-label">City</label>
                    <div class="col-sm-8">
                        <input id="txtBCity" name="txtBCity" type="text" tabindex="3" placeholder="Search with City Name" maxlength="100" class="form-control alpha-space-only" />
                    </div>
                </div>


            </div>
            <div class="col-md-12">

                <div class="form-group col-md-4">
                    <label for="txtTINNo" class="col-sm-4 control-label">TIN No.</label>
                    <div class="col-sm-8">
                        <input id="txtBTINNo" type="text" name="txtBTINNo" tabindex="4" placeholder="Search with Company TIN No." maxlength="20" class="form-control alpha-numeric-only" />
                    </div>
                </div>
           
                <div class="form-group col-md-4">
                    <label for="txtPANNo" class="col-sm-4 control-label">PAN No.</label>
                    <div class="col-sm-8">
                        <input id="txtBPANNo" name="txtBPANNo" type="text" tabindex="5"  placeholder="Search with Company PAN No." maxlength="10" class="form-control alpha-numeric-only" />
                    </div>
                </div>

                <div class="form-group col-md-4">
                    <label for="txtCompanyBranchCode" class="col-sm-4 control-label">Company Branch Code</label>
                    <div class="col-sm-8">
                        <input id="txtCompanyBranchCode" name="txtCompanyBranchCode" type="text" tabindex="5" placeholder="Search with Company Branch Code ." maxlength="10" class="form-control alpha-numeric-only" />
                    </div>
                </div>
            </div>

            <div class="col-md-12">

                <div class="form-group  col-md-4">
                    <label for="txtManufactorLocationCode" class="col-sm-4 control-label">Manufactor Location Code</label>
                    <div class="col-sm-8">
                        <input id="txtManufactorLocationCode" name="txtManufactorLocationCode" type="text" tabindex="18" class="form-control" maxlength="1" />
                    </div>
                </div>
                </div>

                <div class="row col-sm-6 col-lg-12 col-md-12">
                    <div class="col-md-3"></div>
                    <div class="col-md-6">
                        <center>
                            <input class="btn btn-success" value="Search" tabindex="6" alt="Save" id="btnSearch" onclick="SearchCompanyBranch()" type="button" border="0">
                            <input class="btn btn-success" type="button" tabindex="7" id="btnReset" onclick="ClearFields()" value="Reset" />
                            <a class="btn btn-success" id="btnList" tabindex="8" href="@Url.Content("~/CompanyBranch/AddEditCompanyBranch?accessMode=1")">Add More Company Branch</a>
                        </center>
                    </div>
                    <div class="col-md-3"></div>
                </div>
                <div class="row col-sm-6 col-lg-12 col-md-12">
                    <div id="divList" style="margin:30px;margin-top:10px">

                    </div>
                </div>
            </div>
        <!-- Modal -->
        <div class="modal fade" id="alertModel" role="dialog">
            <div class="modal-dialog  modal-sm">
                <!-- Modal content-->
                <div class="modal-content">
                    <div class="modal-header">
                        <button type="button" class="close" data-dismiss="modal">&times;</button>
                        <h4 class="modal-title" id="modelHeader"></h4>
                    </div>
                    <div class="modal-body">
                        <p id="modelText"></p>
                    </div>
                    <div class="modal-footer">
                        <button type="button" class="btn btn-primary  btn-block" data-dismiss="modal">Close</button>
                    </div>
                </div>

            </div>
        </div>
    </form>
</section>
<script src="~/Scripts/Company/ListCompanyBranch.js"></script>
<script src="~/Scripts/jquery-ui.min.js"></script>
<script src="~/Scripts/paging.js"></script>