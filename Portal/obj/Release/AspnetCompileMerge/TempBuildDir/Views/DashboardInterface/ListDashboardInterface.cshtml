@{
    ViewBag.Title = "ListDashboardInterface";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}


<div class="row top-head">
    <div class="col-md-12"><p><b><a style="color:#fff;" href="~/Dashboard/InventoryDashboard">Admin</a> >> List Dashboard Interface </b></p></div>
</div>
<section>
    <form action="#" id="frmAddEditDashboardInterface" class="form-horizontal as" enctype="multipart/form-data" method="post" accept-charset="utf-8">
        <div class="row inner">
            <div class="message">

            </div>
            <div class="col-md-12 ftop">
                <div class="form-group col-md-4">
                    <label for="txtItemName" class="col-sm-4 control-label">Item Name</label>
                    <div class="col-sm-8">
                        <input class="form-control" id="txtItemName" name="txtItemName" tabindex="1" placeholder="Enter Item Name" type="text" maxlength="100" />
                      
                    </div>
                </div>

                <div class="form-group col-md-4">
                    <label for="ddlModuleName" class="col-sm-4 control-label">Module Name</label>
                    <div class="col-sm-8">
                        <select id="ddlModuleName" name="ddlModuleName" tabindex="2" class="form-control">

                            <option value="0">--Select Module--</option>
                            <option value="ACCOUNT">ACCOUNT</option>
                            <option value="ADMIN">ADMIN</option>
                            <option value="CRM">CRM</option>
                            <option value="HR">HR</option>
                            <option value="INVENTORY">INVENTORY</option>
                            <option value="PAYROLL">PAYROLL</option>
                            <option value="PRODUCTION">PRODUCTION</option>
                            <option value="PURCHASE">PURCHASE</option>
                            <option value="SALE">SALE</option>
                        </select>

                    </div>
                </div>

                <div class="form-group col-md-4">
                    <label for="txtContainerName" class="col-sm-4 control-label">Container Name</label>
                    <div class="col-sm-8">
                        <input class="form-control" id="txtContainerName" name="txtContainerName" tabindex="3" placeholder="Enter Container Name" type="text" maxlength="100" />

                    </div>
                </div>

               
             
            </div>
            <div class="col-md-12">

                <div class="form-group col-md-4">
                    <label for="selstatus" class="col-sm-4 control-label">Status</label>
                    <div class="col-sm-8">
                        <select class="form-control" name="ddlStatus" tabindex="4" id="ddlStatus">
                            <option value="">All</option>
                            <option value="1">Active</option>
                            <option value="0">InActive</option>
                        </select>
                    </div>
                </div>

                <div class="form-group col-md-4 ">
                    <label for="ddlCompanyBranch" class="col-sm-4 control-label">Company Branch</label>
                    <div class="col-sm-8">
                        <select id="ddlCompanyBranch" name="ddlCompanyBranch" tabindex="24" class="form-control"></select>
                        <input id="hdnSessionCompanyBranchId" type="hidden" value="@ViewData["CompanyBranchId"]" />
                        <input id="hdnSessionUserID" type="hidden" value="@ViewData["UserId"]" />
                    </div>
                </div>


            </div>
            <div class="row col-sm-6 col-lg-12 col-md-12">
                <div class="col-md-3"></div>
                <div class="col-md-6">
                    <center>
                        <input class="btn btn-success" value="Search" alt="Save" id="btnSearch" tabindex="5" onclick="SearchDashboardInterface();" type="button" border="0">
                        <input class="btn btn-success" type="button" id="btnReset" tabindex="6" onclick="ClearFields();" value="Reset" />
                        <a class="btn btn-success" id="btnList" tabindex="7" href="@Url.Content("~/DashboardInterface/AddEditDashboardInterface?accessMode=1")">Add More Dashboard Interface</a>
                    </center>
                </div>
                <div class="col-md-3"></div>
            </div>
            <div class="row col-sm-6 col-lg-12 col-md-12">
                <div id="divList" style="margin:50px;margin-top:10px">

                </div>
            </div>
        </div>
        <!-- Modal -->
        <div class="modal fade" id="alertModel" role="dialog">
            <div class="modal-dialog  modal-sm">
                <!-- Modal content-->
                <div class="modal-content">
                    <div class="modal-header">
                        <button type="button" class="close" data-dismiss="modal">&times;</button>
                        <h4 class="modal-title" id="modelHeader"></h4>
                    </div>
                    <div class="modal-body">
                        <p id="modelText"></p>
                    </div>
                    <div class="modal-footer">
                        <button type="button" class="btn btn-primary  btn-block" data-dismiss="modal">Close</button>
                    </div>
                </div>

            </div>
        </div>
    </form>
</section>
<script src="~/Scripts/DashboardInterface/ListDashboardInterface.js"></script>
<script src="~/Scripts/jquery-ui.min.js"></script>
<script src="~/Scripts/paging.js"></script>
