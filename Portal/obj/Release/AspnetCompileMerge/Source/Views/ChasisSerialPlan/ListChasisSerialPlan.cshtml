@{
    ViewBag.Title = "List Chasis Serial Plan";
    Layout = "~/Views/Shared/_StoreLayout.cshtml";
}
<style>
    .table {
    width: 150% !important;
    max-width: 150% !important;
    margin-bottom: 20px;
}
</style>
<link href="~/Content/jquery-ui.css" rel="stylesheet" />
<div class="row top-head">
    <div class="col-md-12"><p><b><a style="color:#fff;" href="~/Dashboard/StoreDashboard">Production </a> >> List Chasis Serial Plan </b></p></div>
</div>
<section>
    <form action="#" id="frmSerailPlan" class="form-horizontal as" enctype="multipart/form-data" method="post" accept-charset="utf-8">
        <div class="row inner">
            <div class="message">

            </div>
            <div class="col-md-12 ftop">

                <div class="form-group col-md-4">
                    <label for="txtChasisSerialPlanNo" class="col-sm-4 control-label">Chasis Serial Plan No.</label>
                    <div class="col-sm-8 ">
                        <input class="form-control" id="txtChasisSerialPlanNo" name="txtChasisSerialPlanNo" tabindex="1" placeholder="Search with Chasis Serial Plan No." type="text" maxlength="50" />
                        <input id="hdnSessionCompanyBranchId" type="hidden" value="@ViewData["CompanyBranchId"]" />
                        <input id="hdnSessionUserID" type="hidden" value="@ViewData["UserId"]" />
                    </div>
                </div>

                <div class="form-group col-md-4">
                    <label for="ddlYear" class="col-sm-4 control-label">Year</label>
                    <div class="col-sm-8">
                        <select id="ddlYear" name="ddlYear" tabindex="2" class="form-control">
                            <option value="0">--Select Year--</option>
                            <option value="2017">2017</option>
                            <option value="2018">2018</option>
                            <option value="2019">2019</option>
                            <option value="2020">2020</option>
                            <option value="2021">2021</option>
                            <option value="2022">2022</option>
                            <option value="2023">2023</option>
                            <option value="2024">2024</option>
                            <option value="2025">2025</option>
                            <option value="2026">2026</option>
                            <option value="2027">2027</option>
                            <option value="2028">2028</option>
                            <option value="2029">2029</option>
                            <option value="2030">2030</option>
                            <option value="2031">2031</option>
                        </select>
                    </div>
                </div>
                <div class="form-group  col-md-4">
                    <label for="ddlMonth" class="col-sm-4 control-label">Month</label>
                    <div class="col-sm-8 ">
                        <select id="ddlMonth" name="ddlMonth" tabindex="3" class="form-control">
                            <option value="0">--Select Month--</option>
                            <option value="01">January</option>
                            <option value="02">February</option>
                            <option value="03">March</option>
                            <option value="04">April</option>
                            <option value="05">May</option>
                            <option value="06">June</option>
                            <option value="07">July</option>
                            <option value="08">August</option>
                            <option value="09">September</option>
                            <option value="10">October</option>
                            <option value="11">November</option>
                            <option value="12">December</option>
                        </select>

                    </div>
                </div>
            </div>

            <div class="col-md-12">
                <div class="form-group col-md-4">
                    <label for="txtFromDate" class="col-sm-4 control-label">Chasis Serial Plan From Date</label>
                    <div class="col-sm-8 ">
                        <input class="form-control" id="txtFromDate" name="txtFromDate" tabindex="4" value="@ViewData["fromDate"]" type="text" maxlength="50" />
                        <input id="hdnFromDate" name="hdnFromDate" value="@ViewData["fromDate"]" type="hidden" />
                    </div>
                </div>
                <div class="form-group col-md-4">
                    <label for="txtToDate" class="col-sm-4 control-label">To Date</label>
                    <div class="col-sm-8 ">
                        <input class="form-control" id="txtToDate" name="txtToDate" tabindex="5" value="@ViewData["toDate"]" type="text" maxlength="50" />
                        <input id="hdnToDate" name="hdnToDate" value="@ViewData["toDate"]" type="hidden" />
                    </div>
                </div>
                <div class="form-group  col-md-4">
                    <label for="ddlApprovalStatus" class="col-sm-4 control-label">Chasis Serial Plan Status</label>
                    <div class="col-sm-8">
                        <select id="ddlApprovalStatus" name="ddlApprovalStatus" tabindex="6" class="form-control">
                            <option value="0">--Status--</option>
                            <option value="Final">Final</option>
                            <option value="Draft">Draft</option>
                        </select>
                    </div>
                </div>
            </div>

            <div class="col-md-12">
                <div class="form-group col-md-4 ">
                    <label for="ddlCompanyBranch" class="col-sm-4 control-label">Company Branch&nbsp;<span class="Mandatory">(*)</span></label>
                    <div class="col-sm-8">
                        <select id="ddlCompanyBranch" name="ddlCompanyBranch" tabindex="12" class="form-control"></select>
                    </div>
                </div>
            </div>
            <div class="row col-sm-12 col-lg-12 col-md-12">
                <div class="col-md-3"></div>
                <div class="col-md-6">
                    <center>
                        <input class="btn btn-success" value="Search" alt="Save" id="btnSearch" tabindex="7" onclick="SearchChasisSerialPlan();" type="button" border="0">
                        <input class="btn btn-success" type="button" id="btnReset" tabindex="8" onclick="ClearFields()" value="Reset" /> 
                        <a class="btn btn-success" id="btnList" tabindex="9" href="@Url.Content("~/ChasisSerialPlan/AddEditChasisSerialPlan?accessMode=1")">Add More Chasis Serial Plan</a>
                    </center>
                </div>

             
            </div>
            <div class="row col-sm-6 col-lg-12 col-md-12 ftop">
                <div id="divList">

                </div>
            </div>
        </div>
        <!-- Modal -->
        <div class="modal fade" id="alertModel" role="dialog">
            <div class="modal-dialog  modal-sm">
                <!-- Modal content-->
                <div class="modal-content">
                    <div class="modal-header">
                        <button type="button" class="close" data-dismiss="modal">&times;</button>
                        <h4 class="modal-title" id="modelHeader"></h4>
                    </div>
                    <div class="modal-body">
                        <p id="modelText"></p>
                    </div>
                    <div class="modal-footer">
                        <button type="button" class="btn btn-primary  btn-block" data-dismiss="modal">Close</button>
                    </div>
                </div>

            </div>
        </div>
    </form>
</section>
<script src="~/Scripts/ChasisSerialPlan/ListChasisSerialPlan.js"></script>
<script src="~/Scripts/jquery-ui.min.js"></script>
<script src="~/Scripts/paging.js"></script>
<script src="~/Scripts/Common.js"></script>