                            <div class="col-sm-8">
                                <select id="ddlCompanyBranchForComman" name="ddlCompanyBranchForComman" onchange="CommanMethod();" tabindex="9" class="form-control"></select>
                                <input id="hdnSessionCompanyBranchId" type="hidden" value="@ViewData["CompanyBranchId"]" />
                                <input id="hdnSessionUserID" type="hidden" value="@ViewData["UserId"]" />
                            </div>
                        </div>
                    </div>

                </div>


                <div class="container-fluid-md">
                    <div class="row ftop">
                        <div class="col-sm-6 col-lg-3">
                            <div class="panel panel-metric panel-metric-sm">
                                <div class="panel-body panel-body-primary">
                                    <div class="metric-content metric-icon">
                                        <div class="value" id="total_no_of_Work_Order_Pending_ForSo" style="font-size:22px;">
                                            @*@ViewData["total_no_of_Work_Order_Pending_ForSo"]*@
                                            @ViewData["Box1ItemValue"]
                                        </div>
                                        <div class="icon">
                                            <i class="fa fa-trophy"></i>
                                        </div>
                                        <header>
                                            <a href="@Url.Content("~/SO/ListSO?listStatus=true")" target="_blank" style="color:white;text-decoration:none;">
                                                <h5 class="thin" style="font-weight:600;">
                                                   @ViewData["Box1ItemKey"]
                                                @*Total No. Of SO Pending for WO*@</h5>
                                            </a>
                                            
                                        </header>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="col-sm-6 col-lg-3">
                            <div class="panel panel-metric panel-metric-sm">
                                <div class="panel-body panel-body-success">
                                    <div class="metric-content metric-icon">
                                        <div class="value" id="total_no_of_Work_Order_Pending" style="font-size:22px;">
                                           @*@ViewData["total_no_of_Work_Order_Pending"]*@
                                            @ViewData["Box2ItemValue"]
                                            
                                        </div>
                                        <div class="icon">
                                            <i class="fa fa-users"></i>
                                        </div>
