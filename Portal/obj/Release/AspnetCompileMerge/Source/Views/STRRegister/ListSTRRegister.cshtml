@{
    ViewBag.Title = "List STR Register";
    Layout = "~/Views/Shared/_InventoryLayout.cshtml";
}
<link href="~/Content/jquery-ui.css" rel="stylesheet" />
<div class="row top-head">
    <div class="col-md-12"><p><b><a style="color:#fff;" href="~/Dashboard/InventoryDashboard">Inventory </a> >> List STR Register</b></p></div>
</div>
<section>
    <form action="#" id="frmAddEditSTN" class="form-horizontal as" enctype="multipart/form-data" method="post" accept-charset="utf-8">
        <div class="row inner">
            <div class="message">

            </div>
            <div class="col-md-12 ftop">
                <div class="form-group col-md-4">
                    <label for="txtSTRNo" class="col-sm-4 control-label">STR No.</label>
                    <div class="col-sm-8 ">
                        <input class="form-control" id="txtSTRNo" name="txtSTRNo" tabindex="1" placeholder="Search with STR No." type="text" maxlength="50" />
                    </div>
                </div>
                <div class="form-group  col-md-4">
                    <label for="txtGRNo" class="col-sm-4 control-label">GR No.</label>
                    <div class="col-sm-8">
                        <input class="form-control" id="txtGRNo" name="txtGRNo" tabindex="2" placeholder="Search with GR No." type="text" maxlength="100" />
                    </div>
                </div>
       
                <div class="form-group col-md-4">
                    <label for="ddlFromWarehouse" class="col-sm-4 control-label">From Company Branch</label>
                    <div class="col-sm-8">
                        <select id="ddlFromWarehouse" name="ddlFromWarehouse" tabindex="3" class="form-control"></select>
                      

                    </div>
                </div>
               


            </div>
            <div class="col-md-12"> 
                <div class="form-group col-md-4">
                    <label for="ddlToWarehouse" class="col-sm-4 control-label">To Company Branch</label>
                    <div class="col-sm-8">
                        <select id="ddlToWarehouse" name="ddlToWarehouse" tabindex="4" class="form-control"></select>
                        <input id="hdnSessionCompanyBranchId" type="hidden" value="@ViewData["CompanyBranchId"]" />
                        <input id="hdnSessionUserID" type="hidden" value="@ViewData["UserId"]" />

                    </div>
                </div>
                <div class="form-group col-md-4">
                    <label for="txtCreatedBy" class="col-sm-4 control-label">Created By</label>
                    <div class="col-sm-8">
                        <input class="form-control" id="txtCreatedBy" name="txtCreatedBy" tabindex="5" placeholder="Created By User Name" type="text" maxlength="100" />
                        <input type="hidden" value="0" id="hdnCreatedId" />
                    </div>
                </div>
           
                <div class="form-group  col-md-4">
                    <label for="ddlSortBy" class="col-sm-4 control-label">Sort By</label>
                    <div class="col-sm-8">
                        <select id="ddlSortBy" tabindex="6" name="ddlSortBy" class="form-control">
                            <option value="E.STRNo">STR No.</option>
                            <option value="E.STRDate">STR Date</option>
                            <option value="E.VendorName">Vendor Name</option>
                            <option value="E.TotalValue">Total Value</option>
                            <option value="E.CreatedBy">Created By</option>
                            <option value="st.StateName">State Name</option>
                            <option value="E.City">City</option>
                        </select>
                    </div>
                </div>
              
            </div>  
          
            <div class="col-md-12">
                <div class="form-group col-md-4">
                    <label for="ddlSortOrder" class="col-sm-4 control-label">Sort Order</label>
                    <div class="col-sm-8">
                        <select id="ddlSortOrder" tabindex="7" name="ddlSortOrder" class="form-control">
                            <option value="ASC">Ascending</option>
                            <option value="DESC">Desending</option>
                        </select>
                    </div>
                </div>
                <div class="form-group col-md-4">
                    <label for="txtFromDate" class="col-sm-4 control-label">STR From Date</label>
                    <div class="col-sm-8 ">
                        <input class="form-control" id="txtFromDate" name="txtFromDate" tabindex="8" value="@ViewData["fromDate"]" type="text" maxlength="50" />
                        <input id="hdnFromDate" name="hdnFromDate" value="@ViewData["fromDate"]" type="hidden" />
                    </div>
                </div>
                <div class="form-group col-md-4">
                    <label for="txtToDate" class="col-sm-4 control-label">To Date</label>
                    <div class="col-sm-8 ">
                        <input class="form-control" id="txtToDate" name="txtToDate" tabindex="9" value="@ViewData["toDate"]" type="text" maxlength="50" />
                        <input id="hdnToDate" name="hdnToDate" value="@ViewData["toDate"]" type="hidden" />
                    </div>
                </div>
            </div>



            <div class="row col-sm-12 col-lg-12 col-md-12">
                <div class="col-md-3"></div>
                <div class="col-md-6">
                    <center>
                        <input class="btn btn-success" value="Search" alt="Save" tabindex="10" id="btnSearch" onclick="SearchSTRRegister()" type="button" border="0">&nbsp;&nbsp;
                        <input class="btn btn-success" type="button" id="btnReset" tabindex="11" onclick="ClearFields()" value="Reset" /> &nbsp;&nbsp;
                        <input class="btn btn-success" value="Export" alt="Export" tabindex="12" id="btnExport" onclick="Export()" type="button" border="0">
                    </center>
                </div>
                <div class="col-md-3"></div>
            </div>
            <div class="row col-sm-6 col-lg-12 col-md-12">
                <div id="divList">

                </div>
            </div>
        </div>
        <!-- Modal -->
        <div class="modal fade" id="alertModel" role="dialog">
            <div class="modal-dialog  modal-sm">
                <!-- Modal content-->
                <div class="modal-content">
                    <div class="modal-header">
                        <button type="button" class="close" data-dismiss="modal">&times;</button>
                        <h4 class="modal-title" id="modelHeader"></h4>
                    </div>
                    <div class="modal-body">
                        <p id="modelText"></p>
                    </div>
                    <div class="modal-footer">
                        <button type="button" class="btn btn-primary  btn-block" data-dismiss="modal">Close</button>
                    </div>
                </div>

            </div>
        </div>
    </form>
</section>
<script src="~/Scripts/STRRegister/ListSTRRegister.js"></script>
<script src="~/Scripts/jquery-ui.min.js"></script>
<script src="~/Scripts/paging.js"></script>
<script src="~/Scripts/Common.js"></script>
