@{
    ViewBag.Title = "List Invoice Packing List";
    Layout = "~/Views/Shared/_SaleLayout.cshtml";
}
<link href="~/Content/jquery-ui.css" rel="stylesheet" />
<div class="row top-head">
    <div class="col-md-12"><p><b><a style="color:#fff;" href="~/Dashboard/SaleDashboard">Sale </a> >> List Invoice Packing List</b></p></div>
</div>
<section>
    <form action="#" id="frmAddEditInvoicePackingList" class="form-horizontal as" enctype="multipart/form-data" method="post" accept-charset="utf-8">
        <div class="row inner">
            <div class="message">

            </div>
            <div class="col-md-12 ftop">
                <div class="form-group col-md-4">
                    <label for="txtInvoicePackingListNo" class="col-sm-4 control-label">Packing List No.</label>
                    <div class="col-sm-8 ">
                        <input class="form-control" id="txtInvoicePackingListNo" name="txtInvoicePackingListNo" tabindex="1" placeholder="Search with Packing List No." type="text" maxlength="50" />
                        <input id="hdnSessionCompanyBranchId" type="hidden" value="@ViewData["CompanyBranchId"]" />
                        <input id="hdnSessionUserID" type="hidden" value="@ViewData["UserId"]" /> 
                        <input id="hdnTotalPackingList" type="hidden" value="@ViewData["TotalPackingList"]" /> 
                    </div>
                </div>

                <div class="form-group  col-md-4">
                    <label for="txtInvoiceNo" class="col-sm-4 control-label">Invoice No.</label>
                    <div class="col-sm-8">
                        <input class="form-control" id="txtInvoiceNo" name="txtInvoiceNo" tabindex="2" placeholder="Search with Invoice No." type="text" maxlength="50" />
                    </div>
                </div>

                <div class="form-group col-md-4">
                    <label for="ddlPackingListType" class="col-sm-4 control-label">Packing List Type</label>
                    <div class="col-sm-8">
                        <select id="ddlPackingListType" name="ddlPackingListType" tabindex="3" class="form-control"></select>
                    </div>
                </div>
            </div>
            <div class="col-md-12">
                <div class="form-group col-md-4">
                    <label for="ddlCompanyBranch" class="col-sm-4 control-label">Company Branch&nbsp;<span class="Mandatory">(*)</span></label>
                    <div class="col-sm-8">
                        <select id="ddlCompanyBranch" name="ddlCompanyBranch" tabindex="4" class="form-control"></select>
                    </div>
                </div> 
                <div class="form-group  col-md-4">
                    <label for="ddlApprovalStatus" class="col-sm-4 control-label">Packing List Status</label>
                    <div class="col-sm-8">
                        <select id="ddlApprovalStatus" name="ddlApprovalStatus" tabindex="4" class="form-control">
                            <option value="0">--Status--</option>
                            <option value="Final">Final</option>
                            <option value="Draft">Draft</option>
                        </select>
                    </div>
                </div>      
                <div class="form-group col-md-4">
                    <label for="txtSearchCreatedBy" class="col-sm-4 control-label">Created By UserName</label>
                    <div class="col-sm-8 ">
                        <input class="form-control" id="txtSearchCreatedBy" name="txtSearchCreatedBy" tabindex="7" placeholder="Search with Created By User Name" type="text" maxlength="50" />
                    </div>
                </div>
                
                    
            </div>
         <div class="col-md-12">
             <div class="form-group col-md-4">
                 <label for="txtFromDate" class="col-sm-4 control-label">Packing List From Date</label>
                 <div class="col-sm-8 ">
                     <input class="form-control" id="txtFromDate" name="txtFromDate" tabindex="5" value="@ViewData["fromDate"]" type="text" maxlength="50" />
                     <input id="hdnFromDate" name="hdnFromDate" value="@ViewData["fromDate"]" type="hidden" />
                 </div>
             </div>
             <div class="form-group col-md-4">
                 <label for="txtToDate" class="col-sm-4 control-label">To Date</label>
                 <div class="col-sm-8 ">
                     <input class="form-control" id="txtToDate" name="txtToDate" tabindex="6" value="@ViewData["toDate"]" type="text" maxlength="50" />
                     <input id="hdnToDate" name="hdnToDate" value="@ViewData["toDate"]" type="hidden" />
                 </div>
             </div>  
                
            </div>

                <div class="row col-sm-12 col-lg-12 col-md-12">
                    <div class="col-md-3"></div>
                    <div class="col-md-6">
                        <center>
                            <input class="btn btn-success" value="Search" alt="Save" id="btnSearch" tabindex="8" onclick="SearchInvoicePackingList()" type="button" border="0">
                            <input class="btn btn-success" type="button" id="btnReset" tabindex="8" onclick="ClearFields()" value="Reset" />
                            <a class="btn btn-success" id="btnList" tabindex="9" href="@Url.Content("~/InvoicePackingList/AddEditInvoicePackingList?accessMode=1")">Add More Packing List</a>
                        </center>
                    </div>
                    <div class="col-md-3"></div>
                </div>
                <div class="row col-sm-6 col-lg-12 col-md-12">
                    <div id="divList">

                    </div>
                </div>
            </div>
        <!-- Modal -->
        <div class="modal fade" id="alertModel" role="dialog">
            <div class="modal-dialog  modal-sm">
                <!-- Modal content-->
                <div class="modal-content">
                    <div class="modal-header">
                        <button type="button" class="close" data-dismiss="modal">&times;</button>
                        <h4 class="modal-title" id="modelHeader"></h4>
                    </div>
                    <div class="modal-body">
                        <p id="modelText"></p>
                    </div>
                    <div class="modal-footer">
                        <button type="button" class="btn btn-primary  btn-block" data-dismiss="modal">Close</button>
                    </div>
                </div>

            </div>
        </div>
    </form>
</section>
<script src="~/Scripts/InvoicePackingList/ListInvoicePackingList.js"></script>
<script src="~/Scripts/jquery-ui.min.js"></script>
<script src="~/Scripts/paging.js"></script>